<%= semantic_form_for [:admin, @admin_user] do |f| %>
  <%= f.inputs do %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.input :password_confirmation %>
    <%= f.input :roles, :as => :check_boxes, :collection => Role.order("id DESC").all %>
  <% end %>
  <%= f.actions %>
<% end %>
